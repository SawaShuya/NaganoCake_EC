<div class = "row">
	<div class = "col-xs-offset-1">
		<h3>会員一覧</h3>
	</div>
</div>

<div class = "row">
	<div class = "col-xs-2">
	</div>
	<div class = "col-xs-8">
		<table class = "table" style = "border-bottom: solid; border-width: 1px; border-color: #ddd;">
			<thead>
				<tr class = "active">
					<th>会員ID</th>
					<th>氏名</th>
					<th>メールアドレス</th>
					<th>ステータス</th>
				</tr>
			</thead>

			<tbody>
				<% @end_users.each do |end_user| %>
					<tr>
						<td><%= end_user.id %></td>
						<td>
							<%= link_to "#" do %>
								<%= end_user.last_name %> <%= end_user.first_name %>
							<% end %>
						</td>
						<td><%= end_user.email %></td>
						<td>
							<% if end_user.is_unsubscribe == true %>
								<p>退会済</p>
							<% else %>
								<p>有効</p>
							<% end %>
						</td>
					</tr>
				<% end %>
			</tbody>
		</table>
	</div>
	<div class = "col-xs-2">
	</div>
</div>

